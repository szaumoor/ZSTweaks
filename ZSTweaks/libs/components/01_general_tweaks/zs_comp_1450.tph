	PATCH_IF wpn_cat = 16 AND wpn_prof = 96 BEGIN // daggers
        PATCH_MATCH ench
            WITH 0 BEGIN
                LPF weapon_damage_by_class INT_VAR type = 1 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWD0~ END
