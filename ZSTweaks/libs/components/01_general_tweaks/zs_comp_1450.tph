            END 1 BEGIN
                LPF weapon_damage_by_class INT_VAR type = 1 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWD1~ END
                LPF weapon_damage_by_class INT_VAR type = 2 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWZ1~ END
            END 2 BEGIN
                LPF weapon_damage_by_class INT_VAR type = 1 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWD2~ END
                LPF weapon_damage_by_class INT_VAR type = 2 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWZ2~ END
            END 3 BEGIN
                LPF weapon_damage_by_class INT_VAR type = 1 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWD3~ END
                LPF weapon_damage_by_class INT_VAR type = 2 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWZ3~ END
            END 4 BEGIN
