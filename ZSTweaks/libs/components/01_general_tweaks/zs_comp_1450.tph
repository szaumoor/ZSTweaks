                LPF weapon_damage_by_class INT_VAR type = 2 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWZ5~ END
            END 6 BEGIN
                LPF weapon_damage_by_class INT_VAR type = 1 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWD6~ END
                LPF weapon_damage_by_class INT_VAR type = 2 probability1 = 52 STR_VAR class = ~THIEF~ spell = ~ZSTWZ6~ END
            END DEFAULT
                    LPF ITM_STR_NAME RET name = result END
                    PATCH_WARN "[Finesse: Daggers] Interesting, we found a weapon with more than +6 in enchantment: %SOURCE_RES%, %name%"
        END
    END // end daggers
    PATCH_IF wpn_cat = 14 BEGIN // slings/bullets
