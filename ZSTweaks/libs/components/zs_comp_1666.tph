ACTION_IF (VARIABLE_IS_SET ~zs_comp_1666~) BEGIN
  FAIL ~Script already included: zs_comp_1666~
END ELSE BEGIN
  OUTER_SET ~zs_comp_1666~ = 1
END

ACTION_IF is_bg2ee_or_eet BEGIN

  COPY_EXISTING ~BRAC16.ITM~ ~override~
